@model LoginUser // what data type is this view producing?

<div class="text-center">
    <h1 class="display-4">Welcome to Bank Account</h1>
    <h1 class="display-4">Please Login</h1>
</div>

<form asp-action="Login" asp-controller="Home" method="POST">
    <div class="form-group row">
        <span asp-validation-for="LoginEmail"></span>
        <label asp-for="LoginEmail" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="LoginEmail" class="form-control">
        </div>
    </div>
    <div class="form-group row">
        <span asp-validation-for="LoginPassword"></span>
        <label asp-for="LoginPassword"class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="LoginPassword" class="form-control">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Login</button>
        </div>
        <a href="/new-user">Don't have an account?</a>
    </div>
</form>
