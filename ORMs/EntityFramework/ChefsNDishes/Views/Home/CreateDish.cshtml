@model Dish // What data type is this form creating?

<a>Home</a>

<form asp-controller="Home" asp-action="AddDish" method="POST">
    <div class="form-group">
        <span asp-validation-for="Name"></span>
        <label asp-for="Name"></label>
        <input asp-for="Name">
    </div>
    <div class="form-group">
        <span asp-validation-for="ChefId"></span>
        <label asp-for="ChefId"></label>
        <select asp-for="ChefId">
            <option value="none" selected disabled hidden>
                Select a Chef
            </option>
            @foreach (Chef c in @ViewBag.Chefs)
            {
                <option value=@c.ChefId>@c.FirstName @c.LastName</option>
            }
        </select>
    </div>
    <div class="form-group">
        <span asp-validation-for="Calories"></span>
        <label asp-for="Calories"></label>
        <input asp-for="Calories">
    </div>
    <div class="form-group">
        <span asp-validation-for="Tastiness"></span>
        <label asp-for="Tastiness"></label>
        <select asp-for="Tastiness">
            <option value="none" selected disabled hidden>
                Select a number
            </option>
            <option value=1>1</option>
            <option value=2>2</option>
            <option value=3>3</option>
            <option value=4>4</option>
            <option value=5>5</option>
        </select>
    </div>
    <div class="form-group">
        <span asp-validation-for="Description"></span>
        <label asp-for="Description"></label>
        <textarea asp-for="Description"></textarea>
    </div>
    <input type="submit" value="Add new Dish" />
</form>